#!/bin/bash
#SBATCH -A m1411
#SBATCH -t 01:00:00
#SBATCH -N NUM_NODE
#SBATCH -J NUM_PROCS_procs_job
#SBATCH -C cpu
#SBATCH --qos=regular
#SBATCH -e %j.err
#SBATCH --mail-type=ALL
#SBATCH --mail-user=simonzhang@lbl.gov
export CH_TIMER=1
export CH_OUTPUT_INTERVAL=16

input=INPUT_FILE
ProgName=EXECUTABLE_FILE

srun -N NUM_NODE -n NUM_PROCS $ProgName $input >& log
